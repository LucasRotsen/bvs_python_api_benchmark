DROP SEQUENCE DOG_SEQ;

DROP TABLE DOG;

CREATE SEQUENCE DOG_SEQ
    START WITH 1
    INCREMENT BY 1
    NO MAXVALUE
    NO CYCLE
    CACHE 24;

CREATE TABLE DOG (
    ID NUMERIC(10, 0) NOT NULL,
    NAME VARCHAR(50) NOT NULL,
    PRIMARY KEY (ID)
);

INSERT INTO DOG (ID, NAME) VALUES (nextval('DOG_SEQ'), 'Dunga');
